{
  "name": "business",
  "version": "0.0.1",
  "description": "Base Business service for BASE-platform",
  "main": "Business.js",
  "scripts": {
    "postinstall": "node remove-bitcore-lib-dep.js && tsc",
    "test": "npm run remove-bitcore-lib-dep && mocha --timeout 10000 --recursive --require ts-node/register \"./test/**/*Test.ts\"",
    "coverage": "nyc npm run test",
    "coveralls": "npm run coverage && cat coverage/lcov.info | coveralls",
    "remove-bitcore-lib-dep": "node remove-bitcore-lib-dep.js",
    "build": "tsc",
    "start": "node bin/app.js"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "remove-bitcore-lib-dep.js",
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitclave/base-business-js.git"
  },
  "keywords": [
    "Bitclave",
    "Base",
    "Business",
    "JS",
    "Library",
    "CAT"
  ],
  "author": "Bitclave",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitclave/base-business-js/issues"
  },
  "homepage": "https://github.com/bitclave/base-business-js#readme",
  "dependencies": {
    "@bitclave/base-client-js": "^0.1.4",
    "@types/bn.js": "^4.11.1",
    "@types/config": "0.0.34",
    "@types/web3": "^1.0.10",
    "bignumber": "^1.1.0",
    "bignumber.js": "^7.2.1",
    "config": "^1.30.0",
    "express": "^4.16.3",
    "node-fetch": "^2.2.0",
    "typedarray-to-buffer": "^3.1.5",
    "yaeti": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.7",
    "@types/underscore": "^1.8.9",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "bitcore-ecies": "^1.0.3",
    "bitcore-lib": "^0.15.0",
    "bitcore-message": "^1.0.4",
    "bitcore-mnemonic": "^1.5.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "commander": "^2.15.1",
    "coveralls": "^3.0.0",
    "crypto-js": "^3.1.9-1",
    "elliptic": "^6.4.0",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-tx": "^1.3.4",
    "got": "^8.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "node-localstorage": "^1.3.1",
    "nyc": "^11.4.1",
    "scrypt": "^6.0.3",
    "ts-loader": "^4.0.1",
    "ts-node": "^5.0.1",
    "ts-node-dev": "^1.0.0-pre.18",
    "typescript": "^2.7.1",
    "web3": "^1.0.0-beta.34",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  }
}
